{
    "name" : "Daily Task Tracker",
    "description" :  "Create your daily task",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage"
      ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js" 
      },
      "icons" : {
        "16" : "./images/icon2.png",
        "32" : "./images/icon2.png",
        "48" : "./images/icon2.png"
    }
}